    chmod 664 /sys/devices/virtual/htc_sensorhub/sensor_hub/firmware_info
    chmod 664 /sys/devices/virtual/htc_sensorhub/sensor_hub/firmware_version
    chmod 664 /sys/devices/virtual/htc_sensorhub/sensor_hub/flush
